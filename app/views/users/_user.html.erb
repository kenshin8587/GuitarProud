<% if users.any? %>
<table class="ui red table">
  <thead>
    <tr>
      <th>プロフィール画像</th>
      <th>名前</th>
    </tr>
  </thead>
  <tbody>
  <% users.each do |user| %>
    <tr>
      <td><img class="ui avatar image" src="#"></td>
      <td><%= link_to user.name, user_path(user) %></td>
    </tr>
  </tbody>
  <% end %>
</table>
<% end %>
<%= paginate users %>
